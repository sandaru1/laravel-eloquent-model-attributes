{
  "name": "laravel-eloquent-model-attributes",
  "displayName": "Laravel Eloquent Model Attributes",
  "description": "Generate laravel eloquent model attributes automatically",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "license": "MIT",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "php","laravel","eloquent","snippet","attributes"
  ],
  "icon":"images/icon.jpg",
  "repository": {
    "type": "git",
    "url": "https: //github.com/sandaru1/laravel-eloquent-model-attributes.git"
  },
  "bugs": {
    "url": "https://github.com/sandaru1/laravel-eloquent-model-attributes/issues",
    "email": "sandaruwan@gunathilake.com"
  },  
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "laravel-eloquent-model-attributes.insert-attributes",
        "title": "Insert Eloquent Model Attributes"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "mysql2": "^3.2.4"
  }
}
